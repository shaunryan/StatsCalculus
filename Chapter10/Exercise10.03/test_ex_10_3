import unittest
import sys
import import_ipynb
from Find_the_Volume_of_a_Solid_of_Revolution import *

class Test(unittest.TestCase):
    
    def test_f(self):
        self.assertEqual(f(2),-12)

    def test_g(self):
        self.assertEqual(g(2),-3)
    def test_h(self):
        self.assertEqual(h(3),960)
    def test_vol_solid(self):
        self.assertAlmostEqual(vol_solid(h,-1,1),50.265,3)


if __name__ == '__main__':
    unittest.main()
